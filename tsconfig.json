{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es5",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "downlevelIteration": true,
    "declaration": true,
    "outDir": "./lib",
    "strict": true,
    "lib": [ "es5", "es6", "dom", "dom.iterable" ]
  },
  "include": [ "src" ],
  "exclude": [ "node_modules", ]
}